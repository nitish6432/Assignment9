package myPack;
import java.util.*;


public class noDuplicates {//creating a copy of Duplicates using HashSet which will remove duplicates
	
	public static void main(String[] args) {
		
	
	
	 //Creating HashSet and adding elements
	
	HashSet<String> companies =new HashSet<String>();
	
	companies.add("Mphasis");//Adding object in HashSet 
	companies.add("Cognizant");
	companies.add("Mphasis");
	companies.add("TCS");
	companies.add("Wipro");
	companies.add("TCS");
	companies.add("Accenture");
	companies.add("INTEL");
	companies.add("Google");
	companies.add("TCS");
	companies.add("Cognizant");
	
	//Traversing set through Iterator
	
	 Iterator<String> itr=companies.iterator();  
	  while(itr.hasNext()){  
	   System.out.println(itr.next());//Printing output which will contain no duplicates
	  }  
	 }  
	}  
